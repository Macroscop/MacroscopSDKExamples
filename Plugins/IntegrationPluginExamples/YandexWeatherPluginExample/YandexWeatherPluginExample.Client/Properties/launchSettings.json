{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Configurator": {
      "commandName": "Executable",
      "executablePath": "$(MacroscopFolder)\\MacroscopConfigurator.exe",
      "commandLineArgs": "-server 127.0.0.1 -port 8080 -usertype internal -user root"
    },
    "Client": {
      "commandName": "Executable",
      "executablePath": "$(MacroscopFolder)\\MacroscopClient.exe",
      "commandLineArgs": "-server 127.0.0.1 -port 8080 -usertype internal -user root"
    },
    "Standalone": {
      "commandName": "Executable",
      "executablePath": "$(MacroscopFolder)\\Macroscop.exe"
    }
  }
}
